{
  "manifest_version": 3,
  "name": "A11y checker",
  "version": "1.0",
  "description": "Makes visually hidden text visible",
  "icons": {
    "16": "images/eye-16.png",
    "32": "images/eye-32.png",
    "48": "images/eye-48.png",
    "128": "images/eye-128.png"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/eye-16.png",
      "32": "images/eye-32.png",
      "48": "images/eye-48.png",
      "128": "images/eye-128.png"
    }
  },
  "permissions": ["activeTab", "scripting"]
}